.integral-index-container {
  padding-top: 26rpx;
  box-sizing: border-box;
}

.integral-index-container .profile-container {
  display: flex;
  justify-content: center;
  margin-bottom: 32rpx;
}

.integral-index-container .integral-card-list {
  margin: 0 auto;
  width: 654rpx;
  background-color: #fff;
  border-radius: 16rpx;
  overflow: hidden;
}

.integral-index-container .integral-card-list .integral-card-item.last-card-item {
  padding-bottom: 30rpx;
  box-sizing: border-box;
}