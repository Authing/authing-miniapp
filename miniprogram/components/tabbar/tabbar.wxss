.tabbar-container {
  position: fixed;
  bottom: 0;
  left: 0;
  display: flex;
  align-items: center;
  justify-content: space-around;
  width: 100%;
  background-color: #fff;
}

.tabbar-container .tabbar-item {
  flex: 1;
  height: 52rpx;
}

.tabbar-container .tabbar-item .tabbar-icon {
  width: 100%;
  height: 100%;
}